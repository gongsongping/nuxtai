{
  "name": "nuxta",
  "version": "1.0.0",
  "description": "nuxttest",
  "author": "gongsongping",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server.js",
    "prod": "pm2 stop nuxta&&npm run build&&pm2 start npm  --max-memory-restart 100M --name nuxta -- start",
    "generate": "nuxt generate",
    "server": "NODE_ENV=production&&node server.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt generate",
    "deploy": "push-dir --dir=dist --branch=master --cleanup"
  },
  "dependencies": {
    "ajv": "^6.5.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bootstrap-vue": "^2.0.0-rc.2",
    "cors": "^2.8.4",
    "jquery": "^1.9.1",
    "jsonfile": "^4.0.0",
    "nuxt": "^1.4.0",
    "url-parse": "^1.4.1",
    "vue-infinite-loading": "^2.2.3",
    "vue-loader": "^13.7.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "cross-env": "^5.1.6",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^4.8.3",
    "push-dir": "^0.4.1",
    "sass-loader": "^6.0.7"
  }
}
